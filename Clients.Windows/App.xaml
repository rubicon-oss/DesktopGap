<Application x:Class="DesktopGap.Clients.Windows.App"
             xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
             xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml" 
             Startup="Application_Startup" 
             DispatcherUnhandledException="Application_DispatcherUnhandledException">
    
    <Application.Resources>
        <Style x:Key="buttonStyle" TargetType="Control">
            <Setter Property="FontSize" Value="12" />
            <Setter Property="Background" Value="Transparent" />
            <Setter Property="BorderBrush" Value="Transparent" />
        </Style>

        <BitmapImage x:Key="DesktopGapLogo" UriSource="/DesktopGap;component/Resources/logo.png" />
        <BitmapImage x:Key="DefaultAppIcon" UriSource="/DesktopGap;component/Resources/default-app-icon.png" />


    </Application.Resources>
    
    
    
</Application>
